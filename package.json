{
  "name": "todo-full-stack",
  "version": "0.1.0",
  "description": "Todo Full Stack",
  "repository": "https://github.com/piwakawaka-2020/todo-full-stack",
  "main": "server/index.js",
  "scripts": {
    "dev": "run-p watch:client watch:server",
    "watch:client": "webpack --watch",
    "watch:server": "nodemon --watch server",
    "start": "node server",
    "test": "jest --noStackTrace --watchAll",
    "build": "webpack",
    "knex": "knex --knexfile ./server/db/knexfile.js",
    "lint": "eslint --ext .js,.jsx ."
  },
  "author": "EDA",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      [
        "@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "eda",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/react": "^11.2.2",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.2.1",
    "eslint": "^7.14.0",
    "eslint-config-eda": "^0.1.4",
    "jest": "^26.4.1",
    "nock": "^13.0.5",
    "nodemon": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.3",
    "sqlite3": "^5.0.0",
    "style-loader": "^1.2.1",
    "supertest": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.1.5",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "axios": "^0.21.1",
    "connected-react-router": "^6.9.1",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "framer-motion": "^3.2.1",
    "history": "^4.10.1",
    "knex": "^0.21.5",
    "mini-css-extract-plugin": "^0.10.0",
    "npm-run-all": "^4.1.5",
    "pg": "^8.3.2",
    "react-hook-form": "^6.14.2",
    "react-query": "^3.6.0",
    "react-router": "^5.2.0",
    "redux-saga": "^1.1.3",
    "superagent": "^6.0.0"
  }
}
